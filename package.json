{
  "name": "alexa-randomcat-skill",
  "version": "0.0.0",
  "description": "An Alexa skill for the Amazon Echo.",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js test/**/*.js",
    "build": "npm-run-all --parallel build:skill build:utterances",
    "build:skill": "webpack --config config/webpack.config.babel.js",
    "build:utterances": "mkdir -p build && bin/utterances --schema model/schema.json --utterances model/UTTERANCES > build/UTTERANCES",
    "pretest": "npm run build",
    "test": "ava",
    "posttest": "npm run lint",
    "deploy:prod": "serverless deploy --stage production",
    "deploy:dev": "serverless deploy --stage dev"
  },
  "ava": {
    "babel": "inherit",
    "failWithoutAssertions": false
  },
  "devDependencies": {
    "alexa-annotations": "^9.1.7",
    "alexa-request": "^0.3.3",
    "alexa-response": "^4.2.9",
    "alexa-ssml-jsx": "^2.0.3",
    "alexa-utterances": "^0.2.1",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "chai": "^4.0.2",
    "chai-subset": "^1.5.0",
    "eslint": "^4.2.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "nomnom": "^1.8.1",
    "npm-run-all": "^4.0.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "serverless": "^1.17.0",
    "webpack": "^3.1.0"
  },
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "alexa",
    "alexa-skill",
    "echo",
    "skill"
  ]
}
